  "author": "guoxiangwen",
  "license": "MIT",
  "scripts": {
    "start": "ts-node  scripts/start.ts",
    "build:dev": "cross-env ENV=dev ts-node scripts/build.ts",
    "build:test": "cross-env ENV=test ts-node scripts/build.ts",
    "build:uat": "cross-env ENV=uat ts-node scripts/build.ts",
    "build:prd": "cross-env ENV=prd ts-node scripts/build.ts",
    "test": "ts-node scripts/test.ts",
